# Copy to .env and set values locally
# Set to 'false' to disable the in-browser mock API and use the local proxy server
VITE_USE_MOCK_API=false

# Local proxy server (if running with start:server)
GEMINI_API_URL=https://your-gemini-endpoint.example/v1
GEMINI_API_KEY=

# If you run the proxy on a different port, set VITE_DEV_SERVER_URL to point to it (optional)
# VITE_DEV_SERVER_URL=http://localhost:5174

# Firebase Web SDK configuration (replace with real project values in local .env)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=
